{{> header}}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-5">
            <div class="row">
                <div class="col-md-12">
                    <h3>Javascript <span id="editor-js-change-indicator" style="color:white">&bull;</span></h3>
                    <div class="editor" id="editor-js">{{code}}</div>
                </div>
                <div class="col-md-12">
                    <h3>HTML <span id="editor-html-change-indicator" style="color:white">&bull;</span></h3>
                    <div class="editor" id="editor-html">{{html}}</div>
                </div>
            </div>
        </div>
        <div class="col-md-7">
            <h3>Result</h3>
            <iframe id="preview" src="{{framesource}}"></iframe>
        </div>
    </div>
</div>
<script src="https://cdn.jsdelivr.net/ace/1.2.6/min/ace.js" type="text/javascript" charset="utf-8"></script>
<script src="/static/page.js"></script>
{{> footer}}